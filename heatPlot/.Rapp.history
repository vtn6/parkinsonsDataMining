library(gplots)#
mat = matrix( rnorm(25), 5, 5)#
mat[c(1,6,8,11,15,20,22,24)] = NaN#
#
heatmap.2( mat,#
           col = colorpanel(100,"red","yellow","green"),#
           margins = c(12, 22),#
           trace = "none", #
           xlab = "Comparison",#
           lhei = c(2, 8),#
           scale = c("none"),#
           symbreaks = min(mat, na.rm=TRUE),#
           na.color="blue",#
           cexRow = 0.5, cexCol = 0.7,#
           main = "DE genes", #
           dendrogram = "row", #
           Colv = FALSE )
